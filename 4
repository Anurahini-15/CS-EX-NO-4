attribute = 'sepal_length'
plt.boxplot(iris[attribute])
plt.title(f"Box Plot of {attribute}")
plt.xlabel(attribute)
plt.ylabel("Value")
plt.show()
mean = iris[attribute].mean()
median = iris[attribute].median()
std = iris[attribute].std()
q1 = iris[attribute].quantile(0.25)
q3 = iris[attribute].quantile(0.75)
iqr = q3 - q1
print(f"Statistical Data for {attribute}:")
print(f"Mean: {mean:.2f}")
print(f"Median: {median:.2f}")
print(f"Standard Deviation: {std:.2f}")
print(f"Interquartile Range (IQR): {iqr:.2f}")
plt.scatter(iris.index, iris[attribute])
plt.title(f"Dependency Curve of {attribute}")
plt.xlabel("Index")
plt.ylabel(attribute)
plt.show()
